  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/react": "^17.0.21",
    "@types/react-native": "^0.65.0",
    "jest-expo": "~42.0.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "assets"
